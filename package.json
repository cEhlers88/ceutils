  "homepage": "https://github.com/cEhlers88/ceutils#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "glob": "^7.1.6",
    "jest": "^26.4.2",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
