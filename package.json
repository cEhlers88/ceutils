{
  "name": "@cehlers88/ceutils",
  "version": "1.2.35",
  "description": "my javascript utilities",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && npm run minify && npm run build:styles",
    "build:styles":"node-sass ./src/styles/Dialog.scss ./dist/styles/Dialog.css --output-style compressed",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "minify": "webpack",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cEhlers88/ceutils.git"
  },
  "keywords": [
    "utilities"
  ],
  "author": "Christoph Ehlers <Christoph.Ehlers1988@gmx.de>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cEhlers88/ceutils/issues"
  },
  "homepage": "https://github.com/cEhlers88/ceutils#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/react": "^16.14.21",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^6.7.1",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "prettier": "^1.19.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "files": [
    "build/**/*",
    "dist/**/*"
  ],
  "types": "build/index.d.ts",
  "dependencies": {
    "@types/react-dom": "^16.9.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
